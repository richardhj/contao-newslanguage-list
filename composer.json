	"keywords": ["contao", "newslanguage", "news"],
	"type": "contao-bundle",
	"license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Richard Henkenjohann",
			"email": "richardhenkenjohann@googlemail.com",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/richardhj/contao-newslanguage-list/issues",
		"source": "https://github.com/richardhj/contao-newslanguage-list"
